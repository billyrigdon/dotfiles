gsettings set org.gnome.desktop.interface gtk-theme Flat-Remix-GTK-Blue
gsettings set org.gnome.desktop.wm.preferences theme Flat-Remix-GTK-Blue
gsettings set org.gnome.shell.extensions.user-theme name Flat-Remix-Blue-fullPanel
gsettings set org.gnome.desktop.background picture-uri file:////home/billy/Pictures/Wallpapers/light-wallpaper.jpg
